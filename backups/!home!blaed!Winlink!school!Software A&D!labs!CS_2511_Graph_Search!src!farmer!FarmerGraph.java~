package farmer;

import graph.Edge;
import graph.Graph;
import graph.SimpleVertex;
import graph.Vertex;
import java.util.Arrays;

/**
 * This class constructs an undirected graph of vertices representing
 * states of the FWGC problem.
 * The structure of the graph is as shown at:
 * 
 * http://www.d.umn.edu/~tcolburn/cs2511/javalabs/graphs/images/FarmerGraph1.png
 * 
 * The FWGC states corresponding to the vertices is as shown at:
 * 
 * http://www.d.umn.edu/~tcolburn/cs2511/javalabs/graphs/images/FarmerGraph2.png
 * @author Your name here
 */
public class FarmerGraph extends Graph {
    
    private Vertex s1, s2, s3, s4, s5, s6, s7, s8, s9, s10;
    
    public FarmerGraph() {
    
    s1 = new SimpleVertex("west");
    s2 = new SimpleVertex("s1");
    s3 = new SimpleVertex("s1");
    s4 = new SimpleVertex("s1");
    s5 = new SimpleVertex("s1");
    s6 = new SimpleVertex("s1");
    s7 = new SimpleVertex("s1");
    s8 = new SimpleVertex("s1");
    s9 = new SimpleVertex("s1");
    s10 = new SimpleVertex("s1");
    }
    
    public Vertex getS1() {
        return s1;
    }
    
    public Vertex getS2() {
        return s2;
    }
    
    public Vertex getS3() {
        return s3;
    }
    
    public Vertex getS4() {
        return s4;
    }
    
    public Vertex getS5() {
        return s5;
    }
    
    public Vertex getS6() {
        return s6;
    }
    
    public Vertex getS7() {
        return s7;
    }
    
    public Vertex getS8() {
        return s8;
    }
    
    public Vertex getS9() {
        return s9;
    }
    
    public Vertex getS10() {
        return s10;
    }
}
